<odoo>
    <record id="stock_picking_form_inherit_warranty" model="ir.ui.view">
        <field name="name">stock.picking.form.inherit.warranty</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[field[@name='origin']]" position="inside">
                <field name="warranty_mumber_id"/>
            </xpath>
        </field>
    </record>
</odoo>
